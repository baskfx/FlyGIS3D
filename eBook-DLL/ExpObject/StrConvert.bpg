#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = StringConvertLib StrConvertTest
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

StringConvertLib: StringConvertLib.dpr
  $(DCC)

StrConvertTest: StrConvertTest.dpr
  $(DCC)


